@prefix : <https://github.com/andreamust/HaMSE_Ontology/schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://github.com/andreamust/HaMSE_Ontology/schema> .

<https://github.com/andreamust/HaMSE_Ontology/schema> rdf:type owl:Ontology ;
                                                       <http://purl.org/dc/elements/1.1/creator> "Andrea Poltronieri" ;
                                                       <http://purl.org/dc/elements/1.1/license> "This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/)." ;
                                                       <http://purl.org/dc/terms/license> "Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/)." ;
                                                       rdfs:comment "HaMSE (Harmonic, Melodical, Structural and Emotional feauteres ontology)" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/license
<http://purl.org/dc/elements/1.1/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/rights
<http://purl.org/dc/elements/1.1/rights> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/NET/c4dm/timeline.owl#domainTimeLine
<http://purl.org/NET/c4dm/timeline.owl#domainTimeLine> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain <http://purl.org/NET/c4dm/timeline.owl#TimeLineMap> ;
                                                       rdfs:range <http://purl.org/NET/c4dm/timeline.owl#DiscreteTimeLine> ;
                                                       rdfs:comment "Associates a timeline map to its domain timeline."@en ;
                                                       rdfs:label "domain timeline"@en .


###  http://purl.org/NET/c4dm/timeline.owl#rangeTimeLine
<http://purl.org/NET/c4dm/timeline.owl#rangeTimeLine> rdf:type owl:ObjectProperty ;
                                                      rdfs:domain <http://purl.org/NET/c4dm/timeline.owl#TimeLineMap> ;
                                                      rdfs:range <http://purl.org/NET/c4dm/timeline.owl#AbstractTimeLine> ;
                                                      rdfs:comment "Associates a timeline map to its range timeline."@en ;
                                                      rdfs:label "range timeline"@en .


###  http://purl.org/NET/c4dm/timeline.owl#timeline
<http://purl.org/NET/c4dm/timeline.owl#timeline> rdf:type owl:ObjectProperty ;
                                                 rdfs:domain <http://purl.org/NET/c4dm/timeline.owl#Interval> ;
                                                 rdfs:range <http://purl.org/NET/c4dm/timeline.owl#TimeLine> ;
                                                 rdfs:comment """Relates an interval or an instant to the timeline on which it is defined.

The 29th of August, 2007 would be linked through this property to the universal timeline, whereas
\"from 2s to 5s on this particular signal\" would be defined on the signal' timeline."""@en ;
                                                 rdfs:label "timeline"@en .


###  http://purl.org/ontology/mo/arranged_in
<http://purl.org/ontology/mo/arranged_in> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <http://purl.org/ontology/mo/arrangement_of> ;
                                          rdfs:domain <http://purl.org/ontology/mo/MusicalWork> ;
                                          rdfs:range <http://purl.org/ontology/mo/Arrangement> ;
                                          rdfs:comment "Associates a work to an arrangement event where it was arranged"@en ;
                                          rdfs:label "arranged in"@en .


###  http://purl.org/ontology/mo/arrangement_of
<http://purl.org/ontology/mo/arrangement_of> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://purl.org/ontology/mo/Arrangement> ;
                                             rdfs:range <http://purl.org/ontology/mo/MusicalWork> ;
                                             rdfs:comment "Associates an arrangement event to a work"@en ;
                                             rdfs:label "arrangement of"@en .


###  http://purl.org/ontology/mo/composed_in
<http://purl.org/ontology/mo/composed_in> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <http://purl.org/ontology/mo/produced_work> ;
                                          rdfs:domain <http://purl.org/ontology/mo/MusicalWork> ;
                                          rdfs:range <http://purl.org/ontology/mo/Composition> ;
                                          rdfs:comment "Associates a MusicalWork to the Composition event pertaining to its creation. For example, I might use this property to associate the Magic Flute to its composition event, occuring during 1782 and having as a mo:composer Mozart."@en ;
                                          rdfs:label "composed in"@en .


###  http://purl.org/ontology/mo/composer
<http://purl.org/ontology/mo/composer> rdf:type owl:ObjectProperty ;
                                       owl:inverseOf :isComposerOf ;
                                       rdfs:domain <http://purl.org/ontology/mo/Composition> ;
                                       rdfs:range <http://purl.org/ontology/mo/MusicArtist> ;
                                       rdfs:comment "Associates a composition event to the actual composer. For example, this property could link the event corresponding to the composition of the Magic Flute in 1782 to Mozart himself (who obviously has a FOAF profile:-) )."@en ;
                                       rdfs:label "composer"@en .


###  http://purl.org/ontology/mo/conducted
<http://purl.org/ontology/mo/conducted> rdf:type owl:ObjectProperty ;
                                        owl:inverseOf <http://purl.org/ontology/mo/conductor> ;
                                        rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                                        rdfs:range <http://purl.org/ontology/mo/Performance> ;
                                        rdfs:comment "Relates agents to the performances they were conducting"@en ;
                                        rdfs:label "conducted"@en .


###  http://purl.org/ontology/mo/conductor
<http://purl.org/ontology/mo/conductor> rdf:type owl:ObjectProperty ;
                                        rdfs:domain <http://purl.org/ontology/mo/Performance> ;
                                        rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                                        rdfs:comment "Relates a performance to the conductor involved"@en ;
                                        rdfs:label "conductor"@en .


###  http://purl.org/ontology/mo/genre
<http://purl.org/ontology/mo/genre> rdf:type owl:ObjectProperty ;
                                    rdfs:domain [ rdf:type owl:Class ;
                                                  owl:unionOf ( <http://purl.org/ontology/mo/Arrangement>
                                                                <http://purl.org/ontology/mo/Composition>
                                                                <http://purl.org/ontology/mo/MusicalExpression>
                                                                <http://purl.org/ontology/mo/MusicalManifestation>
                                                                <http://purl.org/ontology/mo/MusicalWork>
                                                                <http://purl.org/ontology/mo/Performance>
                                                                <http://purl.org/ontology/mo/Recording>
                                                              )
                                                ] ;
                                    rdfs:range <http://purl.org/ontology/mo/Genre> ;
                                    rdfs:comment "Associates an event (like a performance or a recording) to a particular musical genre. Further version of this property may also include works and scores in the domain."@en ;
                                    rdfs:label "genre"@en .


###  http://purl.org/ontology/mo/instrument
<http://purl.org/ontology/mo/instrument> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <http://purl.org/ontology/mo/Performance> ;
                                         rdfs:range <http://purl.org/ontology/mo/Instrument> ;
                                         rdfs:comment "Relates a performance to a musical instrument involved"@en ;
                                         rdfs:label "instrument"@en .


###  http://purl.org/ontology/mo/key
<http://purl.org/ontology/mo/key> rdf:type owl:ObjectProperty ;
                                  rdfs:domain [ rdf:type owl:Class ;
                                                owl:unionOf ( <http://purl.org/ontology/mo/Movement>
                                                              <http://purl.org/ontology/mo/MusicalWork>
                                                              <http://purl.org/ontology/mo/Performance>
                                                              <http://purl.org/ontology/mo/Signal>
                                                            )
                                              ] ;
                                  rdfs:range <http://purl.org/ontology/mo/Key> ;
                                  rdfs:comment "Indicated the key used by the musicians during a performance, or the key of a MusicalWork. Any of 24 major or minor diatonic scales that provide the tonal framework for a piece of music."@en ;
                                  rdfs:label "key"@en .


###  http://purl.org/ontology/mo/movement
<http://purl.org/ontology/mo/movement> rdf:type owl:ObjectProperty ;
                                       rdfs:domain <http://purl.org/ontology/mo/MusicalWork> ;
                                       rdfs:range <http://purl.org/ontology/mo/Movement> ;
                                       rdfs:comment "Indicates that a musical work has movements."@en ;
                                       rdfs:label "movement"@en .


###  http://purl.org/ontology/mo/performance_of
<http://purl.org/ontology/mo/performance_of> rdf:type owl:ObjectProperty ;
                                             owl:inverseOf <http://purl.org/ontology/mo/performed_in> ;
                                             rdfs:domain <http://purl.org/ontology/mo/Performance> ;
                                             rdfs:range [ rdf:type owl:Class ;
                                                          owl:unionOf ( <http://purl.org/ontology/mo/MusicalWork>
                                                                        <http://purl.org/ontology/mo/Score>
                                                                      )
                                                        ] ;
                                             rdfs:comment "Associates a Performance to a musical work or an arrangement that is being used as a factor in it. For example, I might use this property to attach the Magic Flute musical work to a particular Performance."@en ;
                                             rdfs:label "performance of"@en .


###  http://purl.org/ontology/mo/performed
<http://purl.org/ontology/mo/performed> rdf:type owl:ObjectProperty ;
                                        owl:inverseOf <http://purl.org/ontology/mo/performer> ;
                                        rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                                        rdfs:range <http://purl.org/ontology/mo/Performance> ;
                                        rdfs:comment "Relates agents to the performances they were performing in"@en ;
                                        rdfs:label "performed"@en .


###  http://purl.org/ontology/mo/performed_in
<http://purl.org/ontology/mo/performed_in> rdf:type owl:ObjectProperty ;
                                           rdfs:domain [ rdf:type owl:Class ;
                                                         owl:unionOf ( <http://purl.org/ontology/mo/MusicalWork>
                                                                       <http://purl.org/ontology/mo/Score>
                                                                     )
                                                       ] ;
                                           rdfs:range <http://purl.org/ontology/mo/Performance> ;
                                           rdfs:comment "Associates a Performance to a musical work or an arrangement that is being used as a factor in it. For example, I might use this property to attach the Magic Flute musical work to a particular Performance."@en ;
                                           rdfs:label "performance in"@en .


###  http://purl.org/ontology/mo/performer
<http://purl.org/ontology/mo/performer> rdf:type owl:ObjectProperty ;
                                        rdfs:domain <http://purl.org/ontology/mo/Performance> ;
                                        rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                                        rdfs:comment "Relates a performance to the performers involved"@en ;
                                        rdfs:label "performer"@en .


###  http://purl.org/ontology/mo/produced_score
<http://purl.org/ontology/mo/produced_score> rdf:type owl:ObjectProperty ;
                                             rdfs:domain [ rdf:type owl:Class ;
                                                           owl:unionOf ( <http://purl.org/ontology/mo/Arrangement>
                                                                         <http://purl.org/ontology/mo/Composition>
                                                                       )
                                                         ] ;
                                             rdfs:range <http://purl.org/ontology/mo/Score> ;
                                             rdfs:comment "Associates an arrangement or a composition event to a score product (score here does not refer to a published score, but more an abstract arrangement of a particular work)."@en ;
                                             rdfs:label "produced_score"@en .


###  http://purl.org/ontology/mo/produced_signal
<http://purl.org/ontology/mo/produced_signal> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://purl.org/ontology/mo/Recording> ;
                                              rdfs:range <http://purl.org/ontology/mo/Signal> ;
                                              rdfs:comment "Associates a Recording to the outputted signal."@en ;
                                              rdfs:label "produced signal"@en .


###  http://purl.org/ontology/mo/produced_sound
<http://purl.org/ontology/mo/produced_sound> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://purl.org/ontology/mo/Performance> ;
                                             rdfs:range <http://purl.org/ontology/mo/Sound> ;
                                             rdfs:comment "Associates a Performance to a physical Sound that is being produced by it."@en ;
                                             rdfs:label "produced sound"@en .


###  http://purl.org/ontology/mo/produced_work
<http://purl.org/ontology/mo/produced_work> rdf:type owl:ObjectProperty ;
                                            rdfs:domain <http://purl.org/ontology/mo/Composition> ;
                                            rdfs:range <http://purl.org/ontology/mo/MusicalWork> ;
                                            rdfs:comment "Associates a composition event to the produced MusicalWork. For example, this property could link the event corresponding to the composition of the Magic Flute in 1782 to the Magic Flute musical work itself. This musical work can then be used in particular performances."@en ;
                                            rdfs:label "produced work"@en .


###  http://purl.org/ontology/mo/published_as
<http://purl.org/ontology/mo/published_as> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <http://purl.org/ontology/mo/MusicalExpression> ;
                                           rdfs:range <http://purl.org/ontology/mo/MusicalManifestation> ;
                                           rdfs:comment "Links a musical expression (e.g. a signal or a score) to one of its manifestations (e.g. a track on a particular record or a published score)."@en ;
                                           rdfs:label "published as"@en .


###  http://purl.org/ontology/mo/recorded_as
<http://purl.org/ontology/mo/recorded_as> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <http://purl.org/ontology/mo/records> ;
                                          rdfs:domain <http://purl.org/ontology/mo/Performance> ;
                                          rdfs:range <http://purl.org/ontology/mo/Signal> ;
                                          rdfs:comment "This is a shortcut property, allowing to bypass all the Sound/Recording steps. This property allows to directly link a Performance to the recorded Signal. This is recommended for \"normal\" users. However, advanced users wanting to express things such as the location of the microphone will have to create this shortcut as well as the whole workflow, in order to let the \"normal\" users access simply the, well, simple information:-) ."@en ;
                                          rdfs:label "recorded as"@en .


###  http://purl.org/ontology/mo/recording_of
<http://purl.org/ontology/mo/recording_of> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <http://purl.org/ontology/mo/Recording> ;
                                           rdfs:range <http://purl.org/ontology/mo/Sound> ;
                                           rdfs:comment "Associates a Recording event to a physical Sound being recorded. For example, I might use this property to associate a given recording, done using my cell phone, to the sound produced by a particular performance of the magic flute."@en ;
                                           rdfs:label "recording of"@en .


###  http://purl.org/ontology/mo/records
<http://purl.org/ontology/mo/records> rdf:type owl:ObjectProperty ;
                                      rdfs:domain <http://purl.org/ontology/mo/Signal> ;
                                      rdfs:range <http://purl.org/ontology/mo/Performance> ;
                                      rdfs:comment "This is the inverse of the shortcut property recordedAs, allowing to relate directly a performance to a signal."@en ;
                                      rdfs:label "records"@en .


###  http://purl.org/ontology/mo/sampled_version_of
<http://purl.org/ontology/mo/sampled_version_of> rdf:type owl:ObjectProperty ;
                                                 rdfs:domain <http://purl.org/ontology/mo/DigitalSignal> ;
                                                 rdfs:range <http://purl.org/ontology/mo/AnalogSignal> ;
                                                 rdfs:label "sampled version of"@en .


###  http://purl.org/ontology/mo/time
<http://purl.org/ontology/mo/time> rdf:type owl:ObjectProperty ;
                                   rdfs:domain <http://purl.org/ontology/mo/DigitalSignal> ;
                                   rdfs:range <http://purl.org/NET/c4dm/timeline.owl#DiscreteInterval> ;
                                   rdfs:comment "Associates a Signal to a time object - its actual domain,"@en ;
                                   rdfs:label "time"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#arousesEmotionType
:arousesEmotionType rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isArousedBy ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :MusicologicalFeature ,
                                [ rdf:type owl:Class ;
                                  owl:unionOf ( <http://purl.org/ontology/mo/MusicalWork>
                                                :Segment
                                              )
                                ] ;
                    rdfs:range :EmotionType ;
                    rdfs:comment "Associates the Musical Work with the Emotion Type that the piece arouses."@en ;
                    rdfs:label "arouses emotion"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#belongsToChord
:belongsToChord rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasChordNote ;
                rdfs:domain :SymbolicEvent ;
                rdfs:range :Chord ;
                rdfs:comment "Associates the Symbolic Event to the Chord. It is the inverse property of hasChordNote."@en ;
                rdfs:label "belongs to chord"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#belongsToIntervalPattern
:belongsToIntervalPattern rdf:type owl:ObjectProperty ;
                          owl:inverseOf :isComposedByInterval ;
                          rdfs:domain :MelodicInterval ;
                          rdfs:range :IntervalPattern ;
                          rdfs:comment "Associates a Melodic Interval to a Melodic Pattern the interval belongs to."@en ;
                          rdfs:label "belongs to interval pattern"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#belongsToMelodicPattern
:belongsToMelodicPattern rdf:type owl:ObjectProperty ;
                         rdfs:domain :SymbolicEvent ;
                         rdfs:range :MelodicPattern ;
                         rdfs:comment "Associates a Symbolic Event to a Melodic Pattern."@en ;
                         rdfs:label "belongs to melodic pattern"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#constitutesIntervalPattern
:constitutesIntervalPattern rdf:type owl:ObjectProperty ;
                            owl:inverseOf :hasIntervalPatternConstituent ;
                            rdfs:domain :IntervalPatternConstituent ;
                            rdfs:range :IntervalPattern ;
                            rdfs:comment "Associates an Interval Pattern Constituent to the belonging Interval Pattern. Inverse of hasIntervalPatternConstituent property."@en ;
                            rdfs:label "constitutes interval pattern"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#constitutesMelodicPattern
:constitutesMelodicPattern rdf:type owl:ObjectProperty ;
                           owl:inverseOf :hasMelodicPatternConstituent ;
                           rdfs:domain :MelodicPatternConstituent ;
                           rdfs:range :MelodicPattern ;
                           rdfs:comment "Associates a Melodic Pattern Constituent to the belonging Melodic Pattern."@en ;
                           rdfs:label "constitutes melodic pattern"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#constitutesProgression
:constitutesProgression rdf:type owl:ObjectProperty ;
                        owl:inverseOf :isComposedByChord ;
                        rdfs:domain :Chord ;
                        rdfs:range :ChordProgression ;
                        rdfs:comment "Associates a Chord to the Chord Progression it belongs to."@en ;
                        rdfs:label "constitutes progression"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#constitutesRhythmicPattern
:constitutesRhythmicPattern rdf:type owl:ObjectProperty ;
                            owl:inverseOf :hasRhythmicPatternConstituent ;
                            rdfs:domain :RhythmicPatternConstituent ;
                            rdfs:range :RhythmicPattern ;
                            rdfs:comment "Relates a Rhythmic Pattern Constituent to the belonging Rhythmic Pattern."@en ;
                            rdfs:label "constitutes rhythmic pattern"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#createsInterval
:createsInterval rdf:type owl:ObjectProperty ;
                 owl:inverseOf :intervalCreatedBy ;
                 rdfs:domain :SymbolicEvent ;
                 rdfs:range :MelodicInterval ;
                 rdfs:comment "Associates two consequent Symbolic Events with the Melodic Interval they create."@en ;
                 rdfs:label "creates interval"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#definesPositionOf
:definesPositionOf rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isDefinedAtTimeInterval ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain <http://purl.org/NET/c4dm/timeline.owl#Interval> ;
                   rdfs:range :Position ;
                   rdfs:comment "Associates the Interval defined on a TimeLine with the Position of the Symbolic Event played at this specific interval."@en ;
                   rdfs:label "defines position of"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#definesSegment
:definesSegment rdf:type owl:ObjectProperty ;
                owl:inverseOf :segmentDefinedBy ;
                rdfs:domain <http://purl.org/NET/c4dm/timeline.owl#DiscreteInterval> ;
                rdfs:range :Segment ;
                rdfs:comment "Links an interval defined on a TimeLine to the segment that defines the interval."@en ;
                rdfs:label "defines segment"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#definesStructure
:definesStructure rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isDefinedBySegment ;
                  rdfs:domain :Structure ;
                  rdfs:range :Segment ;
                  rdfs:comment "Associates a Structure to its constituent Segments."@en ;
                  rdfs:label "defines structure"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasAccidental
:hasAccidental rdf:type owl:ObjectProperty ;
               owl:inverseOf :isAccidentalOf ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain :SymbolicEvent ;
               rdfs:range :Accidental ;
               rdfs:comment "Associates a Symbolic Event to its Accidental."@en ;
               rdfs:label "has accidental"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasChordNote
:hasChordNote rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Chord ;
              rdfs:range :SymbolicEvent ;
              rdfs:comment "Associates a Chord to the Symbolic Events it is composed of."@en ;
              rdfs:label "has chord note"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasChordVoice
:hasChordVoice rdf:type owl:ObjectProperty ;
               owl:inverseOf :isChordVoicingOf ;
               rdf:type owl:InverseFunctionalProperty ;
               rdfs:domain :Chord ;
               rdfs:range :ChordVoicing ;
               rdfs:comment "Associates a Chord to its Voicing."@en ;
               rdfs:label "has chord voice"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasDissonance
:hasDissonance rdf:type owl:ObjectProperty ;
               owl:inverseOf :isDissonanceOf ;
               rdf:type owl:FunctionalProperty ,
                        owl:InverseFunctionalProperty ;
               rdfs:domain :MelodicInterval ;
               rdfs:range :Dissonance ;
               rdfs:comment "Associates a Melodic Interval to a Dissonance."@en ;
               rdfs:label "has dissonance"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasDuration
:hasDuration rdf:type owl:ObjectProperty ;
             owl:inverseOf :isDurationOf ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :SymbolicEvent ;
             rdfs:range :SymbolicDuration ;
             rdfs:comment "Associates a Symbolic Event to the Symbolic Duration it has."@en ;
             rdfs:label "has duration"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasDynamic
:hasDynamic rdf:type owl:ObjectProperty ;
            owl:inverseOf :isDynamicOf ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :SymbolicEvent ;
            rdfs:range :Dynamic ;
            rdfs:comment "Associates a Symbolic Event to its Dynamic."@en ;
            rdfs:label "has dynamic"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasFeature
:hasFeature rdf:type owl:ObjectProperty ;
            owl:inverseOf :isFeatureOf ;
            rdfs:domain <http://purl.org/ontology/mo/MusicalWork> ;
            rdfs:range :MusicologicalFeature ;
            rdfs:comment "Associates the mo:Musical Work with the main class of the whole HaMSE Ontology, namely MusicologicalFeatures."@en ;
            rdfs:label "has feature"@en .


###  https://github.com/andreamust/HaMSE_Ontology/schema#hasInstrumentPart
:hasInstrumentPart rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isPartOf ;
                   rdfs:domain :SymbolicRepresentation ;
                   rdfs:range :InstrumentPart ;
                   rdfs:comment "Associates a Symbolic Representationto the Instrment Parts it composed of."@en ;
                   rdfs:label "has part"@en .


